<odoo>
	<record id="view_tree_clasea" model="ir.ui.view">
		<field name="name">view.tree.Prueba</field>
		<field name="model">clasea.demo</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name"/>	
				<field name="Ap"/>
				<field name="Fecha_N"/>
				<field name="Edad"/>
				<field name="Estado"/>
			</tree>
		</field>
	</record>

	

	

	<record id="action_view_clasea" model="ir.actions.act_window">
		<field name="name">VISTA_A</field>
		<field name="res_model">clasea.demo</field>
        <field name="view_mode">tree,form</field>    
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				Crea tu primera tabla de lista de tareas
			</p>
			</field>    
	</record>

	
<menuitem name="Menu Prueba"
            id="menu_prueba_padre"
            groups="NuevoModulo.group_tecnicaldemo"	
            sequence="24"/>

<menuitem id='menu_prueba_hijo' 
				  parent='NuevoModulo.menu_prueba_padre' 
				  action="NuevoModulo.action_view_clasea"
				  sequence="1" />

	
</odoo>
