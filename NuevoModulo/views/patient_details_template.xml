<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="report_patient_detail">
		<t t-call="web.html_container">
			<t t-foreach="docs" t-as="o">
				<t t-call="web.external_layout">
					<div class="page">
						<div class="oe_structure" />
						<table class="table table-sm o_main_table" name="name">
							<thead>
								<tr>
									<th name="th_Nombre" class="text-left">
										<span>
											Nombre
										</span>
									</th>
									<th name="th_ap" class="text-left">
										<span>
											Apellido
										</span>
									</th>
									<th name="th_Fecha" class="text-left">
										<span>
											Fecha
										</span>
									</th>
									<th name="th_edad" class="text-left">
										<span>
											edad
										</span>
									</th>
								</tr>
							</thead>
							
							<tr>
								<td>
									<span t-field="o.name" />
								</td>
								<td>
									<span t-field="o.Ap" />
								</td>
								<td>
									<span t-field="o.Fecha_N" />
								</td>
								<td>
									<span t-field="o.Edad" />
								</td>
							</tr>
							
						</table>
					</div>
				</t>
			</t>
		</t>
	</template>
</odoo>
