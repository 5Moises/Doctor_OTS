<odoo>

<record id="view_tree_claseb" model="ir.ui.view">
		<field name="name">view.tree.Pruebab</field>
		<field name="model">veterinary.demo</field>
        <field name="inherit_id" ref="veterinary.view_veterinary_demo_tree"/>
		<field name="arch" type="xml">
			
            <xpath expr="//field[@name='fecha_time']" position="after">
          <field name="dato1H"/>		
        </xpath>
		</field>
	</record>

    

<record id="action_view_claseb" model="ir.actions.act_window">
		<field name="name">VISTA_B</field>
		<field name="res_model">veterinary.demo</field>        
        <field name="view_mode">tree,form</field>    
        <field name="view_id" ref="view_tree_claseb"/>
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				Crea tu primera tabla de lista de tareas
			</p>
			</field>    
	</record>

    <menuitem id='menu_prueba_hijoB' 
				  parent='NuevoModulo.menu_prueba_padre' 
				  action="NuevoModulo.action_view_claseb"
				  sequence="2" />


</odoo>